# Memberi nilai pada variabel
a = 75
b = 25

# Membuat output dari operasi dasar python
print(a+b) # Untuk operasi penjumlahan
print(a-b) # Untuk operasi pengurangan
print(a*b) # Untuk operasi perkalian
print(a/b) # Untuk operasi pembagian (hasil dapat berupa bilangan desimal)
print(a//b) # Untuk operasi pembagian (hasil hanya berupa bilangan bulat)
print(a%b) # Untuk mencari sisa dari operasi pembagian
print(a**b) # Untuk mencari nilai pangkat
